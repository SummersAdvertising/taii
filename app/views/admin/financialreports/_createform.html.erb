<%= form_for(@financialreport,:url => admin_financialreports_path, :html => { :id => 'createNewFiscalYearForm' , :method => "post" } ) do |f| %>
	<article>
		<div class="list">
		<% if @financialreport.errors.any? %>
	    <div id="error_explanation">
	      <h2><%= pluralize(@financialreport.errors.count, "error") %> prohibited this financialreport from being saved:</h2>
	      <ul>
			      <% @financialreport.errors.full_messages.each do |msg| %>
			        <li><%= msg %></li>
			      <% end %>
	      </ul>
	    </div>
	  <% end %>
	  
	    <table width="100%" border="0" cellspacing="0" cellpadding="0">
				<tr class="list-edit">
	        <td>&nbsp;</td>
	        <td><%= f.text_field :fiscal_year, class: "text", placeholder:"年份/民國" %></td>
	        <td align="right">
	        	<%= link_to '儲存', '#', onclick: "$('#createNewFiscalYearForm').submit();", title: "儲存" %>
	         </td>
	      </tr>
	    </table>
	  </div>
	  
	  <div class="button">
			<a href=<%= admin_financialreports_path %> class="left" > 
   			返回
   			<img src="/images/left.png">
   		</a>
   	</div>
	</article>
<% end %>

